{% if not user.isGuest() %}	<div class="login">		<div class="card"></div>		<div class="corner left_top"></div><div class="corner right_top"></div><div class="corner left_bottom"></div><div class="corner right_bottom"></div>		<div class="caption">{{ user.login }}</div>		<div class="profile_links">			<div><a href="{% url 'CMS.Profile' %}">Персональный кабинет</a></div>			<div><a href="{% url 'CMS.Logout' %}">Выход</a></div>		</div>	</div>{% else %}	<div class="login">		<div class="card"></div>		<div class="corner left_top"></div><div class="corner right_top"></div><div class="corner left_bottom"></div><div class="corner right_bottom"></div>		<div class="caption">Личный кабинет</div>		<form action="{% url 'ComUsers.Login' %}" method="post" id="login_form">			<div class="email">				<div class="left"></div><div class="right"></div><div class="image"></div>				<input type="text" name="login" value="email"  />			</div>			<div class="password">				<div class="left"></div><div class="right"></div><div class="image"></div>				<input type="password" name="passwd" value="password" />				<input type="image" src="http://uato.biz/images/frontend/user_block_submit.png" />			</div>		</form>		<div class="links"><a href="{% url 'ComUsers.Registration' %}">регистрация</a> - <a href="{% url 'Users.Reminder' %}">восстановление пароля</a></div>	</div>	<script type="text/javascript">	$(document).ready(function() {		$('#login_form input[name=username]').click(function() {			if($(this).val()=='email') {				$(this).val('');			}		});		$('#login_form input[name=username]').blur(function() {			if($(this).val()=='') {				$(this).val('email');			}		});		$('#login_form input[name=password]').click(function() {			if($(this).val()=='password') {				$(this).val('');			}		});		$('#login_form input[name=password]').blur(function() {			if($(this).val()=='') {				$(this).val('password');			}		});	});	</script>{% endif %}