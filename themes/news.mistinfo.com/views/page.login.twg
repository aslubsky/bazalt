<section class="page-login">    <div class="page-header">        <h1>{% tr %}Login{% endtr %}</h1>    </div>    {{ form.begin()|raw }}        {{ form['login']|raw }}        {{ form['password']|raw }}        {{ form['remember']|raw }}        {{ form['backUrl']|raw }}        <a href="{% url 'ComUsers.Recovery' %}">{% tr %}Forgot password{% endtr %}</a>        <div class="form-actions">        {{ form['submit']|raw }}        </div>    {{ form.end()|raw }}</section>